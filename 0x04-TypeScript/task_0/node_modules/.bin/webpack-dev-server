#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/johnian/batcave/0x04-TypeScript/task_0/node_modules/.pnpm/webpack-dev-server@3.11.3_webpack-cli@3.3.12_webpack@4.47.0__webpack@4.47.0_webpack-cli@3.3.12_/node_modules/webpack-dev-server/bin/node_modules:/home/johnian/batcave/0x04-TypeScript/task_0/node_modules/.pnpm/webpack-dev-server@3.11.3_webpack-cli@3.3.12_webpack@4.47.0__webpack@4.47.0_webpack-cli@3.3.12_/node_modules/webpack-dev-server/node_modules:/home/johnian/batcave/0x04-TypeScript/task_0/node_modules/.pnpm/webpack-dev-server@3.11.3_webpack-cli@3.3.12_webpack@4.47.0__webpack@4.47.0_webpack-cli@3.3.12_/node_modules:/home/johnian/batcave/0x04-TypeScript/task_0/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/johnian/batcave/0x04-TypeScript/task_0/node_modules/.pnpm/webpack-dev-server@3.11.3_webpack-cli@3.3.12_webpack@4.47.0__webpack@4.47.0_webpack-cli@3.3.12_/node_modules/webpack-dev-server/bin/node_modules:/home/johnian/batcave/0x04-TypeScript/task_0/node_modules/.pnpm/webpack-dev-server@3.11.3_webpack-cli@3.3.12_webpack@4.47.0__webpack@4.47.0_webpack-cli@3.3.12_/node_modules/webpack-dev-server/node_modules:/home/johnian/batcave/0x04-TypeScript/task_0/node_modules/.pnpm/webpack-dev-server@3.11.3_webpack-cli@3.3.12_webpack@4.47.0__webpack@4.47.0_webpack-cli@3.3.12_/node_modules:/home/johnian/batcave/0x04-TypeScript/task_0/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../webpack-dev-server/bin/webpack-dev-server.js" "$@"
else
  exec node  "$basedir/../webpack-dev-server/bin/webpack-dev-server.js" "$@"
fi
