#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/johnian/batcave/0x04-TypeScript/task_0/node_modules/.pnpm/ts-jest@24.3.0_jest@24.9.0/node_modules/ts-jest/node_modules:/home/johnian/batcave/0x04-TypeScript/task_0/node_modules/.pnpm/ts-jest@24.3.0_jest@24.9.0/node_modules:/home/johnian/batcave/0x04-TypeScript/task_0/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/johnian/batcave/0x04-TypeScript/task_0/node_modules/.pnpm/ts-jest@24.3.0_jest@24.9.0/node_modules/ts-jest/node_modules:/home/johnian/batcave/0x04-TypeScript/task_0/node_modules/.pnpm/ts-jest@24.3.0_jest@24.9.0/node_modules:/home/johnian/batcave/0x04-TypeScript/task_0/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ts-jest/cli.js" "$@"
else
  exec node  "$basedir/../ts-jest/cli.js" "$@"
fi
