#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/johnian/batcave/0x04-TypeScript/task_0/node_modules/.pnpm/webpack-cli@3.3.12_webpack@4.47.0/node_modules/webpack-cli/bin/node_modules:/home/johnian/batcave/0x04-TypeScript/task_0/node_modules/.pnpm/webpack-cli@3.3.12_webpack@4.47.0/node_modules/webpack-cli/node_modules:/home/johnian/batcave/0x04-TypeScript/task_0/node_modules/.pnpm/webpack-cli@3.3.12_webpack@4.47.0/node_modules:/home/johnian/batcave/0x04-TypeScript/task_0/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/johnian/batcave/0x04-TypeScript/task_0/node_modules/.pnpm/webpack-cli@3.3.12_webpack@4.47.0/node_modules/webpack-cli/bin/node_modules:/home/johnian/batcave/0x04-TypeScript/task_0/node_modules/.pnpm/webpack-cli@3.3.12_webpack@4.47.0/node_modules/webpack-cli/node_modules:/home/johnian/batcave/0x04-TypeScript/task_0/node_modules/.pnpm/webpack-cli@3.3.12_webpack@4.47.0/node_modules:/home/johnian/batcave/0x04-TypeScript/task_0/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../webpack-cli/bin/cli.js" "$@"
else
  exec node  "$basedir/../webpack-cli/bin/cli.js" "$@"
fi
