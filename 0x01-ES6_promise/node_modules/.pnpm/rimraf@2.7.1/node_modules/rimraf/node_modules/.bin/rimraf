#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/johnian/batcave/0x01-ES6_promise/node_modules/.pnpm/rimraf@2.7.1/node_modules/rimraf/node_modules:/home/johnian/batcave/0x01-ES6_promise/node_modules/.pnpm/rimraf@2.7.1/node_modules:/home/johnian/batcave/0x01-ES6_promise/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/johnian/batcave/0x01-ES6_promise/node_modules/.pnpm/rimraf@2.7.1/node_modules/rimraf/node_modules:/home/johnian/batcave/0x01-ES6_promise/node_modules/.pnpm/rimraf@2.7.1/node_modules:/home/johnian/batcave/0x01-ES6_promise/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin.js" "$@"
else
  exec node  "$basedir/../../bin.js" "$@"
fi
